{
	"preset": "ts-jest",
	"testEnvironment": "node",
	"roots": ["src"],
	"collectCoverageFrom": ["**/*.ts"],
	"coveragePathIgnorePatterns": [
	],
	"coverageDirectory": "coverage",
	"coverageReporters": ["lcov"],
	"coverageThreshold": {
		"global": {
			"statements": 100,
			"branches": 100,
			"functions": 100,
			"lines": 100
		}
	},
	"clearMocks": true
}
